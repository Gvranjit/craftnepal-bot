{
  "name": "craftnepal-bot",
  "version": "3.0.0",
  "description": "CraftNepal Bot re-done using ts, and prisma",
  "main": "./build/index.js",
  "repository": "https://github.com/Gvranjit/craftnepal-bot.git",
  "author": "Gaurav Ranjit",
  "license": "MIT",
  "scripts": {
    "build": "swc ./src -d ./build",
    "typecheck": "tsc --noEmit",
    "dev": "ts-node ./src/index",
    "fast-dev": "npm run build && nodemon build/index",
    "serve": "pm2 start ./build/index.js --name='craftnepal-bot'"
  },
  "dependencies": {
    "concurrently": "^7.3.0",
    "discord.js": "^14.0.2",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "joi": "^17.6.0",
    "pm2": "^5.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.218",
    "@types/node": "*",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
