{
  "name": "craftnepal-bot",
  "version": "3.0.0",
  "description": "CraftNepal Bot re-done using ts, and prisma",
  "main": "./build/index.js",
  "repository": "https://github.com/Gvranjit/craftnepal-bot.git",
  "author": "Gaurav Ranjit",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --respawn ./server/",
    "build:next": "next build",
    "build:server": "npm run clean && tsc --project tsconfig.server.json",
    "build": "concurrently \"npm run build:server\" \"npm run build:next\"",
    "clean": "rimraf build",
    "start": "dotenv -v NODE_ENV=production node build/index.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.18.1",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@discordjs/builders": "^1.0.0",
    "@discordjs/rest": "^1.0.0",
    "@prisma/client": "^4.1.0",
    "concurrently": "^7.3.0",
    "discord-api-types": "^0.36.2",
    "discord.js": "^14.0.2",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "joi": "^17.6.0",
    "pm2": "^5.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.218",
    "@types/node": "*",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3"
  }
}
